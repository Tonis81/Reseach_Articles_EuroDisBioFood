@article{Julvez2011,
	abstract = {Objective: We studied the associations between Attention Deficit Hyperactivity Disorder (ADHD) symptoms and the neurobehavioral status in two population-based birth cohorts. Methods: Children (n=467) were assessed by psychologists and teachers for neuropsychological functioning (McCarthy Scales, MCSA), inattention-hyperactivity symptoms (ADHD-DSM-IV form list) and social behavior (California Preschool Social Competence Scale, CPSCS). Regression models were used with covariate adjustment. Results: Sixteen percent of children had ADHD-DSM-IV symptoms. MCSA scores were linearly associated with ADHD symptom scores (general cognitive Beta. =-0.6 [-1.0; -0.3] per symptom), specifically inattention scores (general cognitive Beta. =-1.8 [-2.3; -1.2]). CPSCS scores were associated with ADHD symptoms (Beta. =-2.19 [-2.5; -1.9]). MCSA scores of executive function, perceptive-performance and quantitative sub-areas had stronger associations with ADHD symptoms. Conclusions: Preschooler ADHD symptoms are associated with concurrent decrements in neurocognitive and social competence functioning. The association patterns are similar to those found in older children with ADHD symptomology (Marks et al., 2005 [36], Seidman, 2006 [46], Sonuga-Barke et al., 2003 [48], Yochman et al., 2006 [53]). {\textcopyright} 2010 Elsevier Masson SAS.},
	author = {Julvez, J. and Forns, M. and Ribas-Fit{\'{o}}, N. and Torrent, M. and Sunyer, J.},
	date-added = {2023-01-23 05:00:51 +0100},
	date-modified = {2023-01-23 05:00:51 +0100},
	doi = {10.1016/j.eurpsy.2010.03.013},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Julvez et al. - 2011 - Attention behavior and hyperactivity and concurrent neurocognitive and social competence functioning in 4-year-ol.pdf:pdf},
	isbn = {1778-3585 (Electronic)$\backslash$n0924-9338 (Linking)},
	issn = {09249338},
	journal = {European Psychiatry},
	keywords = {ADHD symptoms,Cognitive development,Population-based birth cohort,Preschoolers,Social competence},
	number = {6},
	pages = {381--389},
	pmid = {20620026},
	title = {Attention behavior and hyperactivity and concurrent neurocognitive and social competence functioning in 4-year-olds from two population-based birth cohorts},
	volume = {26},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.1016/j.eurpsy.2010.03.013}}

