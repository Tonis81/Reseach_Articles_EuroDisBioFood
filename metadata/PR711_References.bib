	issn = {1476069X},
	journal = {Environmental Health: A Global Access Science Source},
	keywords = {Exposure-response,Health risk estimation,Mixtures,Multiple exposures,Variable selection},
	number = {1},
	pages = {1--10},
	publisher = {Environmental Health},
	title = {Statistical software for analyzing the health effects of multiple concurrent exposures via Bayesian kernel machine regression},
