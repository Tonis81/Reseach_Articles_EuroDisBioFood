	issn = {19882904},
	journal = {Spanish Journal of Psychology},
	keywords = {Basque,MSCA,neuropsychological assessment,preschool,validation},
	pages = {1--10},
	title = {Neuropsychological Assessment at Preschool Age: Adaptation and Validation of the McCarthy Scales of Children's Abilities to 4 Year-old Basque-speaking Children},
