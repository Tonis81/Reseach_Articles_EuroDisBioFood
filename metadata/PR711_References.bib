@article{Carrico2015,
	abstract = {In risk evaluation, the effect of mixtures of environmental chemicals on a common adverse outcome is of interest. However, due to the high dimensionality and inherent cor-relations among chemicals that occur together, the traditional methods (e.g. ordinary or logistic regression) suffer from collinearity and variance inflation, and shrinkage meth-ods have limitations in selecting among correlated components. We propose a weighted quantile sum (WQS) approach to estimating a body burden index, which identifies " bad actors " in a set of highly correlated environmental chemicals. We evaluate and charac-terize the accuracy of WQS regression in variable selection through extensive simulation studies through sensitivity and specificity (i.e., ability of the WQS method to select the bad actors correctly and not incorrect ones). We demonstrate the improvement in accu-racy this method provides over traditional ordinary regression and shrinkage methods (lasso, adaptive lasso, and elastic net). Results from simulations demonstrate that WQS regression is accurate under some environmentally relevant conditions, but its accuracy decreases for a fixed correlation pattern as the association with a response variable dimin-ishes. Nonzero weights (i.e., weights exceeding a selection threshold parameter) may be used to identify bad actors; however, components within a cluster of highly correlated ac-tive components tend to have lower weights, with the sum of their weights representative of the set. Supplementary materials accompanying this paper appear on-line.},
	author = {Carrico, Caroline and Gennings, Chris and Wheeler, David C. and Factor-Litvak, Pam},
	date-added = {2022-09-26 07:49:53 +0200},
	date-modified = {2022-09-26 07:49:53 +0200},
	doi = {10.1007/s13253-014-0180-3},
	file = {:Users/tonisignes/Library/Application Support/Mendeley Desktop/Downloaded/Carrico et al. - 2015 - Characterization of Weighted Quantile Sum Regression for Highly Correlated Data in a Risk Analysis Setting.pdf:pdf},
	issn = {15372693},
	journal = {Journal of Agricultural, Biological, and Environmental Statistics},
	keywords = {Correlation,Nonlinear model,Subset selection,Variable selection,WQS},
	number = {1},
	pages = {100--120},
	title = {Characterization of Weighted Quantile Sum Regression for Highly Correlated Data in a Risk Analysis Setting},
	volume = {20},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1007/s13253-014-0180-3}}

